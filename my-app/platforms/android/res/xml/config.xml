<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegaptips.googleoauth" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <preference name="AndroidLaunchMode" value="singleTop" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="TelephoneNumber">
        <param name="android-package" value="com.finder.device.TelephoneNumber" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SharedPrefs">
        <param name="android-package" value="com.finder.sharedprefs.SharedPrefs" />
        <param name="onload" value="true" />
    </feature>
    <name>FinderTracking</name>
    <description>
        This is the FINDER GPS Tracking Application which helps everybody to track from anywhere and anytime.
    </description>
    <author email="dev@callback.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <access origin="*" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="true" />
    <plugin name="Device" value="org.apache.cordova.Device" />
    <plugin name="Device" value="org.apache.cordova.Geolocation" />
    <plugin name="DeviceInfo" value="com.finder.device.DeviceInfo" />
    <plugin name="DeviceInfo" value="com.finder.device.TelephoneNumber" />
    <plugin name="SharedPrefs" value="com.finder.sharedprefs.SharedPrefs" />
    <plugin name="LocationInfo" value="com.finder.location.LocationInfo" />
</widget>
